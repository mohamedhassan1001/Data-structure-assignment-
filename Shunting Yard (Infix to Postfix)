#include <stdio.h>
#include <ctype.h>

int prec(char c) {
    if (c == '+' || c == '-') return 1;
    if (c == '*' || c == '/') return 2;
    return 0;
}

void infixToPostfix(char* exp) {
    char stack[100];
    int top = -1;
    for (int i = 0; exp[i]; i++) {
        if (isalnum(exp[i])) printf("%c", exp[i]);
        else {
            while (top != -1 && prec(stack[top]) >= prec(exp[i]))
                printf("%c", stack[top--]);
            stack[++top] = exp[i];
        }
    }
    while (top != -1) printf("%c", stack[top--]);
}

int main() {
    char exp[] = "a+b*c-d";
    infixToPostfix(exp);
    return 0;
}
